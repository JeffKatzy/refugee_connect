%section.row#hero.six.sixths
  .row.two.sixths.centered
    Upload Teachers Page
    = form_for @bookpage, :html => {:multipart => true} do |f|
      %p
        = f.label :page_number
        = f.text_field :page_number
      %p
        = f.label :teachers_page
        = f.select :teachers_page, [['Teachers Page', true], ['Not', false]]
      %p
        = f.attachinary_file_field :photos
      %p
        = f.submit